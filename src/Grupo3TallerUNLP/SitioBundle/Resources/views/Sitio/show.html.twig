{% extends '::dashboard.html.twig' %}

{% block content%}
    <div class="page-header">
        <h3>Eliminar Sitio</h3>
    </div>

    <dl class="dl-horizontal">

                <dt>Nombre</dt>
                <dd>{{ entity.nombre }}</dd>

                <dt>Url</dt>
                <dd>{{ entity.url }}</dd>

                <dt>Grado de aceptaci√≥n</dt>
                <dd>{{entity.gradoAceptacion}}</dd>

			    <dt>Grupo</dt>
			    <dd>{{entity.grupo}}</dd>
				
				{% if plantilla %}
					<dt>Plantillas</dt>
					{% for p in plantilla %}
						<dd>{{ p}}</dd>
					{% endfor %}
				{% endif %}
				
    </dl>


    <div class="col-md-offset-1">
        <a href="{{ path('sitio') }}" class="btn btn-primary">Volver</a>
    </div>
{% endblock %}
