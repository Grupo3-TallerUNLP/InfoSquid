{% extends '::dashboard.html.twig' %}

{% block content %}
    <div class="page-header">
        <h1>Eliminar Oficina</h1>
    </div>

     <dl class="dl-horizontal">
        <dt>Nombre</dt>
        <dd>{{ entity.nombre }}</dd>
       
	   <dt>Descripcion</dt>
       <dd>{{ entity.descripcion }}</dd>
                        
		<dt>Ubicacion</dt>
        <dd>{{ entity.ubicacion }}</dd>
        
		<dt>Director</dt>
        <dd>{{ entity.director }}</dd>
            
    </dl>
   
   <div class="col-md-offset-1">
        {{ form_start(delete_form) }}
            {{ form_widget(delete_form.submit) }}
            <a href="{{ path('oficina') }}" class="btn btn-default">Cancelar</a>
        {{ form_end(delete_form) }}
    </div>
{% endblock %}
