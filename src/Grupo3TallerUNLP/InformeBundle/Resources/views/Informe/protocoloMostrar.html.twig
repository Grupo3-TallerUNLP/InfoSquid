{% extends '::dashboard.html.twig' %}

{% block javascripts %}
    {{ parent() }}
    {% javascripts 
		'@Grupo3TallerUNLPInformeBundle/Resources/public/highcharts.js'
		'@Grupo3TallerUNLPInformeBundle/Resources/public/exporting.js' %}
		 <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
		<script type="text/javascript">
		{{ chart(chart) }}
	</script>
{% endblock javascripts %}

{% block content %}
    <div class="page-header">
        <h3>Top N de protocolos mas utilizados</h3>
    </div>
	<div class="alert alert-info alert-dismissible">
			<button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">&times;</span><span class="sr-only">Cerrar</span></button>
			{% if (filtros|length > 0) %}
				Filtrando por:
				{% for filtro in filtros %}
					{{ filtro }}
				{% endfor %}
			{% endif %}
		</div>


	

	{% if 	resultados	%}
		<div id="linechart" style="min-width: 400px; height: 400px; margin: 0 auto"></div>
	{% else %}
		<label class="col-lg-7"> No se encontraron resultados </label>
	{% endif %}
{% endblock %}